{
  "name": "picostack",
  "type": "module",
  "version": "3.0.0",
  "description": "Minimal Web8 Framework",
  "main": "index.js",
  "files": [
    "index.js",
    "simple-kernel.js",
    "simple-rpc.js",
    "modem56.js"
  ],
  "scripts": {
    "lint": "standard",
    "test": "node test.js",
    "modem": "browserify modem56.js -s Modem56 -o modem56.web.js",
    "debug": "node inspect test.js",
    "types": "(rm *.ts || true) && tsc --skipLibCheck --emitDeclarationOnly --allowJs --checkJs --declaration --removeComments --lib es2022 --target es2022 --moduleResolution nodenext --module nodenext index.js",
    "prepublishOnly": "standard && yarn test && yarn modem"
  },
  "types": "index.d.ts",
  "repository": {
    "type": "git",
    "url": "git://https://github.com/telamon/picostack"
  },
  "author": "Tony Ivanov", "license": "AGPL-3.0-or-later",
  "bugs": {
    "url": "https://github.com/telamon/picostack"
  },
  "homepage": "https://github.com/telamon/picostack",
  "devDependencies": {
    "@types/web": "^0.0.151",
    "brittle": "^3.6.1",
    "browserify": "^17.0.0",
    "debug": "^4.3.6",
    "hypercore-protocol": "^8.0.7",
    "hyperswarm-web": "^2.2.0",
    "memory-level": "^1.0.0",
    "standard": "^17.1.0"
  },
  "dependencies": {
    "@telamon/picostore": "../picostore",
    "cborg": "^4.2.3",
    "picofeed": "^8.0.3",
    "piconet": "^3.2.1",
    "piconuro": "^1.0.1",
    "picorepo": "^2.2.3"
  }
}
